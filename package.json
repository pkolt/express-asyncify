{
  "name": "express-asyncify",
  "version": "2.1.3",
  "description": "Easy support async/await to express",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "repository": "pkolt/express-asyncify",
  "scripts": {
    "test": "node --loader ts-node/esm --test --no-warnings ./src/*.test.ts",
    "build-esm": "tsc",
    "build-cjs": "tsc -p ./tsconfig.cjs.json && ts-rename --cjs ./dist/cjs",
    "build-types": "tsc -p ./tsconfig.types.json",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build-esm && npm run build-cjs && npm run build-types",
    "prepare": "npm run build",
    "format": "prettier --check src",
    "format-fix": "prettier --write src",
    "release": "release-it"
  },
  "keywords": [
    "express",
    "async",
    "await"
  ],
  "bugs": {
    "url": "https://github.com/pkolt/express-asyncify/issues",
    "email": "pkoltyshev@gmail.com"
  },
  "author": "Pavel Koltyshev <pkoltyshev@gmail.com>",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/supertest-as-promised": "^2.0.42",
    "prettier": "^3.2.5",
    "release-it": "^17.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^10.9.2",
    "ts-rename": "^0.2.0",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "express": "^4.19.2"
  }
}
