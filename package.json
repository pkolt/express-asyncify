{
  "name": "express-asyncify",
  "version": "2.0.0",
  "description": "Easy support async/await to express",
  "type": "module",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "repository": "pkolt/express-asyncify",
  "scripts": {
    "test": "node --loader ts-node/esm --test --no-warnings ./src/*.test.ts",
    "build-esm": "tsc",
    "build-cjs": "tsc -p ./tsconfig.cjs.json",
    "build-types": "tsc -p ./tsconfig.types.json",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build-esm && npm run build-cjs",
    "prepare": "npm run build",
    "format": "prettier --check src",
    "format-fix": "prettier --write src",
    "release": "release-it"
  },
  "keywords": [
    "express",
    "async",
    "await"
  ],
  "bugs": {
    "url": "https://github.com/pkolt/express-asyncify/issues",
    "email": "pkoltyshev@gmail.com"
  },
  "author": "Pavel Koltyshev <pkoltyshev@gmail.com>",
  "files": [
    "README.md",
    "LICENSE.md",
    "dist/*"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.14",
    "@types/supertest-as-promised": "^2.0.38",
    "express": "4.18.2",
    "prettier": "^2.8.1",
    "release-it": "^15.5.1",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "express": "^4.18.2"
  }
}
